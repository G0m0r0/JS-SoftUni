{{> header}}
<div class="container home wrapper  my-md-5 pl-md-5">
    <div class="profile home-text col-md-6 text-center col-lg">
      <img class="profile-img" src="./images/user.png" />
      <div class="profile-info">
        <p>Username: <small>John</small></p>
        <p class="infoType">Has {{dataOfProfile.countIdeas}} ideas =)</p>
        {{#each dataOfProfile.nameIdeas}}
        <p>{{this}}</p>
        {{else}}
        <p>No ideas yet.</p>
        {{/each}}
      </div>
    </div>
  </div>
  {{> footer}}